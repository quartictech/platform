{
  "name": "weyl-frontend",
  "description": "Quartic Map Frontend",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "1.16.0",
    "@blueprintjs/datetime": "1.14.1",
    "babel-polyfill": "6.13.0",
    "chroma-js": "1.2.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "express": "4.14.0",
    "history": "3.0.0",
    "immutable": "3.8.1",
    "invariant": "2.2.1",
    "javascript-natural-sort": "0.7.1",
    "jquery": "3.1.1",
    "lodash": "4.14.1",
    "mapbox-gl": "0.32.1",
    "mapbox-gl-draw": "0.16.0",
    "moment": "2.17.1",
    "plottable": "2.6.0",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.7",
    "react-sizeme": "2.1.3",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4",
    "roboto-fontface": "0.6.0",
    "sanitize.css": "4.1.0",
    "underscore": "1.8.3",
    "warning": "3.0.0",
    "whatwg-fetch": "1.0.0"
